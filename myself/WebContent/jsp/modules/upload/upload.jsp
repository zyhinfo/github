<%@ page language="java" contentType="text/html; charset=UTF-8"%><%@ include file="/jsp/base/core/taglibs.jsp"%><!DOCTYPE html><html>	<head>		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">		<script type="text/javascript">			var fileSuffixs = {".et":".et",".txt":".txt", ".xls":".xls",".xlsx":".xlsx", 							   ".dbf":".dbf", ".csv":".csv"};			$(function(){				init_page();				$("#uploadbutton").click(function(){					var fileName = $("#file1").val().substr($("#file1").val().lastIndexOf("\\")+1);					if(fileName.length == 0){						warn_alert("请先选择上传的文件");					}else {						var fileSuffix=fileName.substring(fileName.lastIndexOf("."));						if(fileSuffix.toLowerCase() in fileSuffixs){							$("#uploadForm").attr("action","${ctx}/UploadAction.action?method=upload");							$("#uploadForm").submit();						}else{							warn_alert("只能上传<br> .txt, .xlsx, .xls, .dbf, .csv, .et<br>类型的文件");				        }					}									});				$("#back").click(function(){					$("#paramForm").attr("action","${ctx}${upload.backUrl}");					$("#paramForm").submit();				});			});					</script>	</head>	<body style="overflow-x:hidden; padding:2px;">		<form id="paramForm" action="" method="post"></form>		<form id="uploadForm" action="" enctype="multipart/form-data" method="post" class="self-form">			<!-- 公共参数 -->			<input type="hidden" name="backUrl" value="${upload.backUrl }">			<input type="hidden" name="endUrl" value="${upload.endUrl }">			<input type="hidden" name="templateTableName" value="${upload.templateTableName }">			<input type="hidden" name="importTableName" value="${upload.importTableName }">    		<table class="self-table">    			<tr>    				<td width="90px">数据名称</td>    				<td><input type="text" id="dataName" name="dataName" value=""></td>    			</tr>    			<tr>    				<td>上传文件</td>    				<td><input type="file" id="file1" name="file1" value=""></td>    			</tr>    			<tr>    				<td colspan="2">    					<input type="button" id="uploadbutton" value="上传">    					<c:if test="${upload.backUrl != ''}">    						<input type="button" id="back" value="返回">    					</c:if>    				</td>    			</tr>    		</table>    	</form>	</body></html>
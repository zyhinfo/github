<%@ page language="java" import="java.util.*"  contentType="text/html; charset=UTF-8" %><%@ include file="/jsp/base/core/taglibs.jsp"%><html>	<head>		<title>系统错误！</title>		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">		<script type="text/javascript">			function errorBack(){				var tab = window.parent.tab;				var tabid = tab.getSelectedTabItemID();				var text = $("li[tabid=" + tabid + "] a", tab.tab.links.ul).text();				$.post("${ctx}/ErrorLogAction.action?method=addErrorLog&anticache="						+ Math.floor(Math.random() * 10000),{					menuName: text,					errorSummary: $("#summary").val(),					errorDesc: $("#desc").val()					},function(info){						if (info.info == undefined) {							info = eval("(" + info + ")");						}						if(info.info=="ok"){							$("#errorBack").attr("disabled","true");							$.ligerDialog.success('反馈成功');						}				});			}		</script>	</head>	<body>		<input id="summary" type="hidden" value="${ex }">		<input id="desc" type="hidden" value="${error }">		<h1>${ex }</h1>		<br>		${error }		<hr>		<input type="button" id="errorBack" onclick="errorBack()" value="错误反馈">	</body></html>
<%@ page language="java" contentType="text/html; charset=UTF-8"%><%@ include file="/jsp/base/core/taglibs.jsp"%><html>	<head>		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">		<style type="text/css">             body{ padding:5px; margin:0; padding-bottom:15px;}            #layout1{  width:100%;margin:0; padding:0;  }              .l-page-top{ height:80px; background:#f8f8f8; margin-bottom:3px;}            h4{ margin:20px;}            .l-link{ display:block; height:26px; line-height:26px; padding-left:10px; text-decoration:underline; color:#333;}        </style>		<script type="text/javascript">	        $(function (){	        	$("#layout1").ligerLayout({					leftWidth: 150,					allowLeftCollapse:false,					allowLeftResize:false});	        	$(".l-link").click(function(){	        		var win=document.getElementById("showFrame").contentWindow;	        		win.scrollwindow($(this).attr("attrId"));	        	});	        });		</script>	</head>	<body>		<div id="layout1">            <div position="left">            	<a class="l-link" href="#" attrId="1">基本信息</a>            	<a class="l-link" href="#" attrId="2">教育信息</a>            	<a class="l-link" href="#" attrId="3">工作经历</a>            	<a class="l-link" href="#" attrId="4">培训经历</a>            	<a class="l-link" href="#" attrId="5">奖罚经历</a>            	<a class="l-link" href="#" attrId="6">年度休假</a>            </div>            <div position="center" title="">            	<div tabid="home" title="我的主页" >                	<iframe id="showFrame" frameborder="0" width="100%" height="100%"                 		src="${ctx }/PersonalFileAction.action?method=showPersonalFile&archiveId=${archiveId}"></iframe>            	</div>            </div>          </div> 	</body></html>
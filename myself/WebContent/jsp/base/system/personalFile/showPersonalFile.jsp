<%@ page language="java" contentType="text/html; charset=UTF-8"%><%@ include file="/jsp/base/core/taglibs.jsp"%><html>	<head>		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">		<script type="text/javascript" src="${ctx }/js/base/system/personalFile/personalFile.js?m=1"></script>		<style type="text/css"> 			h2{			    background-color: #7F8FB1;				color: #fff;				font-size:13px;				text-align: left;				padding: 3px 6px 3px 6px;				border: 1px #7F8FB1 solid;			}			hr{				border:1px #cccccc dotted;			}			.title{				background:#DBDBDB;				width: 90px;			}        </style>		<script type="text/javascript">			$(function (){				init_page();				projectName='${ctx}';				var titId = '${titId}';				if(titId != undefined && titId != null){					scrollwindow(titId);				}				var msg = '${msg}';				if(msg == "err"){					warn_alert("请上传小于2M的头像");				}			});			function scrollwindow(id) {				window.scroll(0, $("#title"+id).offset().top);			}			function clickImg(){				//点击图片出发文件选择框				$("#uploadImg").click();			}			function openImg(){				//var imgFile=$("#uploadImg").val();				$("#uploadForm").attr("action","${ctx}/PersonalFileAction.action?method=uploadImg");				$("#uploadForm").submit();			}		</script>	</head>	<body>		<div id="title1">			<h2>基本信息</h2>			<div style="position: relative;">				<table class="self-table" width="550px" border="1" cellpadding='0' cellspacing='0'>	    			<tr >	    				<td class="title">姓名</td>	    				<td width="350px" id="title1_userName">${archiveInfo.USER_NAME }</td>	    				<td rowspan="4" align="center">	    					<a onclick="clickImg();">	    					<img title="头像" width="80px" height="80px" src="data:image/jpg;base64,${archiveInfo.PHOTO }">	    					</a>	    				</td>	    			</tr>	    			<tr>	    				<td class="title">职位</td>	    				<td id="title1_positionName">${archiveInfo.POSITION_NAME }</td>	    			</tr>	    			<tr>	    				<td class="title">出生日期</td>	    				<td id="title1_birthday">${archiveInfo.BIRTHDAY }</td>	    			</tr>	    			<tr>	    				<td class="title">性别</td>	    				<td id="title1_sex">${archiveInfo.SEX }</td>	    			</tr>	    			<tr>	    				<td class="title">民族</td>	    				<td colspan="2" id="title1_natonalName">${archiveInfo.NATONAL_NAME }</td>	    			</tr>	    			<tr>	    				<td class="title">个人电话</td>	    				<td colspan="2" id="title1_tel">${archiveInfo.TEL }</td>	    			</tr>	    			<tr>	    				<td class="title">家庭电话</td>	    				<td colspan="2" id="title1_homeTel">${archiveInfo.HOMETEL }</td>	    			</tr>	    			<tr>	    				<td class="title">家庭地址</td>	    				<td colspan="2" id="title1_homeAddr">${archiveInfo.HOME_ADDR }</td>	    			</tr>	    			<tr>	    				<td class="title">职称</td>	    				<td colspan="2" id="title1_jobTitle">${archiveInfo.JOB_TITLE }</td>	    			</tr>	    			<tr>	    				<td class="title">部门</td>	    				<td colspan="2" id="title1_orgId">idsp</td>	    			</tr>	    			<tr>	    				<td class="title">政治面貌</td>	    				<td colspan="2" id="title1_zzmm">${archiveInfo.ZZMM }</td>	    			</tr>	    		</table>	    		<div style="width:30px;z-index: 1;position:absolute;top:5px;right: 10px;">    				<a onclick="updateArchiveInfo('title1_')">修改</a>    			</div>    		</div>		</div>				<div id="title2" >			<h2>教育信息</h2>			<c:forEach items="${educationList }" var="item" varStatus="ges">				<c:if test="${ges.count != 1 }"><hr></c:if>				<div style="position: relative;">				<table class="self-table" width="550" border="1" cellpadding='0' cellspacing='0'>	    			<tr>	    				<td class="title">毕业院校</td>	    				<td id="title2_${ges.count}_school">${item.SCHOOL }</td>	    			</tr>	    			<tr>	    				<td class="title">专业</td>	    				<td id="title2_${ges.count}_profession">${item.PROFESSION }</td>	    			</tr>	    			<tr>	    				<td class="title">学历</td>	    				<td id="title2_${ges.count}_education">${item.EDUCATION }</td>	    			</tr>	    			<tr>	    				<td class="title">学位</td>	    				<td id="title2_${ges.count}_degreeName">${item.DEGREE_NAME }</td>	    			</tr>	    			<tr>	    				<td class="title">入学时间</td>	    				<td id="title2_${ges.count}_startDate">${item.START_DATE }</td>	    			</tr>	    			<tr>	    				<td class="title">毕业时间</td>	    				<td id="title2_${ges.count}_endDate">${item.END_DATE }</td>	    			</tr>	    				    		</table>	    		<div style="width:30px;z-index: 1;position:absolute;top:5px;right: 10px;">    				<a onclick="updateArchiveEducation('title2_${ges.count}_','${item.ID }')">修改</a>    			</div>	    	</div>		</c:forEach>		<input type="button" class="l-button" onclick="addArchiveEducation()" value="新增">				<div id="title3">			<h2>本行工作经历</h2>			<c:forEach items="${jobList }" var="item" varStatus="ges">				<c:if test="${ges.count != 1 }"><hr></c:if>				<div style="position: relative;">					<table class="self-table" width="550" border="1" cellpadding='0' cellspacing='0'>		    			<tr>		    				<td class="title">公司名称</td>		    				<td id="title3_${ges.count}_companyName">${item.COMPANY_NAME }</td>		    			</tr>		    			<tr>		    				<td class="title">公司地址</td>		    				<td id="title3_${ges.count}_companyAddr">${item.COMPANY_ADDR }</td>		    			</tr>		    			<tr>		    				<td class="title">入职时间</td>		    				<td id="title3_${ges.count}_addDate">${item.ADD_DATE }</td>		    			</tr>		    			<tr>		    				<td class="title">离职时间</td>		    				<td id="title3_${ges.count}_leaveDate">${item.LEAVE_DATE }</td>		    			</tr>		    			<tr>		    				<td class="title">职位</td>		    				<td><p id="title3_${ges.count}_position">${item.POSITION }</p></td>		    			</tr>		    			<tr>		    				<td valign="top" class="title">工作职责</td>		    				<td id="title3_${ges.count}_jobDes">${item.JOB_DES }</td>		    			</tr>		    			<tr>		    				<td valign="top" class="title">离职原因</td>		    				<td id="title3_${ges.count}_leaveReason">${item.LEAVE_REASON }</td>		    			</tr>		    		</table>		    		<div style="width:30px;z-index: 1;position:absolute;top:5px;right: 10px;">	    				<a onclick="updateArchiveJob('title3_${ges.count}_','${item.ID }')">修改</a>	    			</div>		    	</div>    		</c:forEach>		</div>		<input type="button" class="l-button" onclick="addArchiveJob()" value="新增">				<div id="title4">			<h2>培训经历</h2>			<c:forEach items="${trainList }" var="item" varStatus="ges">				<c:if test="${ges.count != 1 }"><hr></c:if>				<div style="position: relative;">					<table class="self-table" width="550" border="1" cellpadding='0' cellspacing='0'>		    			<tr>		    				<td class="title">培训机构</td>		    				<td id="title4_${ges.count}_school">${item.SCHOOL }</td>		    			</tr>		    			<tr>		    				<td valign="top" class="title">培训内容</td>		    				<td id="title4_${ges.count}_trainDesc">${item.TRAIN_DESC }</td>		    			</tr>		    			<tr>		    				<td class="title">入学时间</td>		    				<td id="title4_${ges.count}_startDate">${item.START_DATE }</td>		    			</tr>		    			<tr>		    				<td class="title">毕业时间</td>		    				<td id="title4_${ges.count}_endDate">${item.END_DATE }</td>		    			</tr>		    		</table>		    		<div style="width:30px;z-index: 1;position:absolute;top:5px;right: 10px;">	    				<a onclick="updateArchiveTrain('title4_${ges.count}_','${item.ID }')">修改</a>	    			</div>		    	</div>    		</c:forEach>		</div>		<input type="button" class="l-button" onclick="addArchiveTrain()" value="新增">		<div id="title5">			<h2>奖罚经历</h2>			<c:forEach items="${rewardList }" var="item" varStatus="ges">				<c:if test="${ges.count != 1 }"><hr></c:if>				<div style="position: relative;">					<table class="self-table" width="550" border="1" cellpadding='0' cellspacing='0'>		    			<tr>		    				<td class="title">奖罚原因</td>		    				<td id="title5_${ges.count}_jfReason">${item.JF_REASON }</td>		    			</tr>		    			<tr>		    				<td class="title">奖罚明细</td>		    				<td id="title5_${ges.count}_jfDetail">${item.JF_DETAIL }</td>		    			</tr>		    			<tr>		    				<td class="title">奖罚类型</td>		    				<td id="title5_${ges.count}_jfType">${item.JF_TYPE }</td>		    			</tr>		    			<tr>		    				<td class="title">奖罚时间</td>		    				<td id="title5_${ges.count}_jfDate">${item.JF_DATE }</td>		    			</tr>		    		</table>		    		<div style="width:30px;z-index: 1;position:absolute;top:5px;right: 10px;">	    				<a onclick="updateArchiveReward('title5_${ges.count}_','${item.ID }')">修改</a>	    			</div>		    	</div>    		</c:forEach>		</div>		<input type="button" class="l-button" onclick="addArchiveReward()" value="新增">		<div id="title6">			<h2>年度休假</h2>			<c:forEach items="${holidayList }" var="item" varStatus="ges">				<c:if test="${ges.count != 1 }"><hr></c:if>				<div style="position: relative;">					<table class="self-table" width="550" border="1" cellpadding='0' cellspacing='0'>		    			<tr>		    				<td class="title">休假原因</td>		    				<td id="title6_${ges.count}_reason">${item.REASON }</td>		    			</tr>		    			<tr>		    				<td class="title">休假类型</td>		    				<td id="title6_${ges.count}_isType">${item.IS_TYPE }</td>		    			</tr>		    			<tr>		    				<td class="title">开始时间</td>		    				<td id="title6_${ges.count}_startDate">${item.START_DATE }</td>		    			</tr>		    			<tr>		    				<td class="title">结束时间</td>		    				<td id="title6_${ges.count}_endDate">${item.END_DATE }</td>		    			</tr>		    		</table>		    		<div style="width:30px;z-index: 1;position:absolute;top:5px;right: 10px;">	    				<a onclick="updateArchiveHoliday('title6_${ges.count}_','${item.ID }')">修改</a>	    			</div>		    	</div>    		</c:forEach>		</div>		<input type="button" class="l-button" onclick="addArchiveHoliday()" value="新增">				<!-- =========================维护信息============================ -->				<!-- 修改基本信息 -->    	<div id="targetArchiveInfo" style="display:none;">    		<form class="self-form">        	<table class="self-table">    			<tr >    				<td width="90px">姓名</td>    				<td><input type="text" id="userName" name="userName" value=""></td>    			</tr>    			<tr>    				<td>职位</td>    				<td><input type="text" id="positionName" name="positionName" value=""></td>    			</tr>    			<tr>    				<td>出生日期</td>    				<td><input type="text" id="birthday" name="birthday" ltype="date" value=""></td>    			</tr>    			<tr>    				<td>性别</td>    				<td>    					<select id="sex" name="sex">    						<option value="女">女    						<option value="男">男    					</select>    				</td>    			</tr>    			<tr>    				<td>民族</td>    				<td><input type="text" id="natonalName" name="natonalName" value=""></td>    			</tr>    			<tr>    				<td>个人电话</td>    				<td><input type="text" id="tel" name="tel" ltype="int" value=""></td>    			</tr>    			<tr>    				<td>家庭电话</td>    				<td><input type="text" id="homeTel" name="homeTel" ltype="int" value=""></td>    			</tr>    			<tr>    				<td>家庭地址</td>    				<td><input type="text" id="homeAddr" name="homeAddr" value=""></td>    			</tr>    			<tr>    				<td>职称</td>    				<td><input type="text" id="jobTitle" name="jobTitle" value=""></td>    			</tr>    			<tr>    				<td>部门</td>    				<td><input type="text" id="orgId" name="orgId" value=""></td>    			</tr>    			<tr>    				<td>政治面貌</td>    				<td>    					<select id="zzmm" name="zzmm">    						<option value="非团员">非团员    						<option value="团员">团员    					</select>    				</td>    			</tr>    		</table>    		</form>    	</div>		<!-- 修改教育信息 -->		<div id="targetArchiveEducation" style="display:none;">    		<form class="self-form">        	<table class="self-table">    			<tr >    				<td width="90px">毕业院校</td>    				<td><input type="text" id="school" name="school" value=""></td>    			</tr>    			<tr >    				<td width="90px">专业</td>    				<td><input type="text" id="profession" name="profession" value=""></td>    			</tr>    			<tr >    				<td width="90px">学历</td>    				<td><input type="text" id="education" name="education" value=""></td>    			</tr>    			<tr >    				<td width="90px">学位</td>    				<td><input type="text" id="degreeName" name="degreeName" value=""></td>    			</tr>    			<tr >    				<td width="90px">入学时间</td>    				<td><input type="text" id="education_startDate" name="education_startDate" ltype="date" value=""></td>    			</tr>    			<tr >    				<td width="90px">毕业时间</td>    				<td><input type="text" id="education_endDate" name="education_endDate" ltype="date" value=""></td>    			</tr>    		</table>    		</form>    	</div>    	    	<!-- 工作经历 -->    	<div id="targetArchiveJob" style="display:none;">    		<form class="self-form">        	<table class="self-table">    			<tr>    				<td width="90px">公司名称</td>    				<td><input type="text" id="companyName" name="companyName" value=""></td>    			</tr>    			<tr>    				<td>公司地址</td>    				<td><input type="text" id="companyAddr" name="companyAddr" value=""></td>    			</tr>    			<tr>    				<td>入职时间</td>    				<td><input type="text" id="addDate" ltype="date" name="addDate" value=""></td>    			</tr>    			<tr>    				<td>离职时间</td>    				<td><input type="text" id="leaveDate" ltype="date" name="leaveDate" value=""></td>    			</tr>    			<tr>    				<td>职位</td>    				<td><input type="text" id="position" name="position" value=""></td>    			</tr>    			<tr>    				<td valign="top">工作职责</td>    				<td>    					<textarea rows="4" cols="40" name="jobDes" id="jobDes"></textarea>    				</td>    			</tr>    			<tr>    				<td valign="top">离职原因</td>    				<td>    					<input type="text" id="leaveReason" name="leaveReason" value="">    				</td>    			</tr>    		</table>    		</form>    	</div>    	    	<!-- 培训经历 -->    	<div id="targetArchiveTrain" style="display:none;">    		<form class="self-form">        	<table class="self-table">    			<tr>    				<td width="90px">培训机构</td>    				<td><input type="text" id="train_school" name="train_school" value=""></td>    			</tr>    			<tr>    				<td valign="top">培训内容</td>    				<td><textarea rows="4" cols="40" name="train_trainDesc" id="train_trainDesc"></textarea></td>    			</tr>    			<tr>    				<td>入学时间</td>    				<td><input type="text" ltype="date" id="train_startDate" name="train_startDate" value=""></td>    			</tr>    			<tr>    				<td>毕业时间</td>    				<td><input type="text" ltype="date" id="train_endDate" name="train_endDate" value=""></td>    			</tr>    		</table>    		</form>    	</div>    	    	<!-- 奖罚 -->    	<div id="targetArchiveReward" style="display:none;">    		<form class="self-form">        	<table class="self-table">    			<tr>    				<td width="90px">奖罚原因</td>    				<td><input type="text" id="jfReason" name="jfReason" value=""></td>    			</tr>    			<tr>    				<td valign="top">奖罚明细</td>    				<td><textarea rows="4" cols="40" name="jfDetail" id="jfDetail"></textarea></td>    			</tr>    			<tr>    				<td>奖罚类型</td>    				<td>    					<select id="jfType" name="jfType">    						<option value="奖励">奖励    						<option value="惩罚">惩罚    					</select>    				</td>    			</tr>    			<tr>    				<td>奖罚时间</td>    				<td><input type="text" ltype="date" id="jfDate" name="jfDate" value=""></td>    			</tr>    		</table>    		</form>    	</div>    	    	<!-- 年度休假 -->    	<div id="targetArchiveHoliday" style="display:none;">    		<form class="self-form">        	<table class="self-table">    			<tr>    				<td width="90px">休假原因</td>    				<td><input type="text" id="reason" name="reason" value=""></td>    			</tr>    			<tr>    				<td>休假类型</td>    				<td>    					<select id="isType" name="isType">    						<option value="事假">事假    						<option value="病假">病假    						<option value="年假">年假    						<option value="其他">其他    					</select>    				</td>    			</tr>    			<tr>    				<td>开始时间</td>    				<td><input type="text" ltype="date" id="holiday_startDate" name="holiday_startDate" value=""></td>    			</tr>    			<tr>    				<td>结束时间</td>    				<td><input type="text" ltype="date" id="holiday_endDate" name="holiday_endDate" value=""></td>    			</tr>    		</table>    		</form>    	</div>    			<input type="hidden" id="id" value="">				<!-- 上传头像用到的标签 -->		<form id="uploadForm" method="post" action="" enctype="multipart/form-data" >			<input type="file" id="uploadImg" name="uploadImg" onchange="openImg();" style="visibility:hidden">			<input type="hidden" id="archiveId" name="archiveId" value="${archiveInfo.ARCHIVE_ID}">		</form>		<iframe id="frameFile" name="frameFile" style="display: none;"></iframe>		<!-- 上传头像用到的标签 -->						<div style="height: 450px;"></div>	</body></html>
<%@ page language="java" contentType="text/html; charset=UTF-8"%><%@ include file="/jsp/base/core/taglibs.jsp"%><!DOCTYPE html><html>	<head>		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">		<script type="text/javascript">			$(function(){				init_page();			});			function f_save(){                var param = {					menuId: $("#menuId").val(),					ownMenuId: $("#ownMenuId").val(),					menuName: liger.get("menuName").getValue(),					menuDesc: $("#menuDesc").val(),					linkUrl: liger.get("linkUrl").getValue(),					menuIcon: liger.get("menuIcon").getValue(),					seqNum: liger.get("seqNum").getValue()				}				$.post("${ctx}/MenuAction.action?method=editMenu",param,function(info){					if (info.info == undefined) {						info = eval("(" + info + ")");					}					if(info.info=='ok'){						window.parent.refresh();					}else{						error_alert("${ctx}","修改菜单失败",mapToString(param));					}				});							}		</script>	</head>	<body style="overflow-x:hidden; padding:2px;">    	<div id="form1" class="self-form">	       	<table class="self-table">    			<tr>    				<td width="90px">上级菜单</td>    				<td><input type="text" id="ownMenuName" disabled="disabled" value="${bean.ownMenuName}"></td>    			</tr>    			<tr>    				<td width="90px">菜单名称</td>    				<td><input type="text" id="menuName" value="${bean.menuName}"></td>    			</tr>    			<tr>    				<td width="90px">菜单描述</td>    				<td>    					<textarea rows="5" cols="40" id="menuDesc" name="menuDesc">${bean.menuDesc}</textarea>    				</td>    			</tr>    			<tr>    				<td width="90px">链接地址</td>    				<td><input type="text" id="linkUrl" value="${bean.linkUrl}"></td>    			</tr>    			<tr>    				<td width="90px">显示图标</td>    				<td><input type="text" id="menuIcon" value="${bean.menuIcon}"></td>    			</tr>    			<tr>    				<td width="90px">显示顺序</td>    				<td><input type="text" id="seqNum" value="${bean.seqNum}" ltype="int"></td>    			</tr>    		</table>    		<input type="button" onclick="f_save()" class="l-button" value="保存">    	</div>    	<input type="hidden" id="ownMenuId" value="${bean.ownMenuId }">		<input type="hidden" id="menuId" value="${bean.menuId }">	</body></html>
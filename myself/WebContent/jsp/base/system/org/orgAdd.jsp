<%@ page language="java" contentType="text/html; charset=UTF-8"%><%@ include file="/jsp/base/core/taglibs.jsp"%><!DOCTYPE html><html>	<head>		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">		<script type="text/javascript">			$(function(){				init_page();			});			function f_save(){				var form = liger.get('form1');	            if (form.valid()) {	            	var param = {						ownOrgId: $("#ownOrgId").val(),						orgLev: $("#orgLev").val(),						orgName: liger.get("orgName").getValue()					};					$.post("${ctx}/OrgAction.action?method=addOrg",param,function(info){						if (info.info == undefined) {							info = eval("(" + info + ")");						}						if(info.info=='ok'){							window.parent.refresh();							liger.get("orgName").setValue("");						}else{							error_alert("${ctx}","新增部门失败",mapToString(param));						}					});	            }else {	                form.showInvalid();	            }			}		</script>	</head>	<body style="overflow-x:hidden; padding:2px;">    	<form id="form1" class="self-form">    		<table class="self-table">    			<tr>    				<td width="90px">上级部门</td>    				<td><input type="text" id="ownMenuName" disabled="disabled" value="${ownOrgName}"></td>    			</tr>    			<tr>    				<td width="90px">部门名称</td>    				<td><input type="text" id="orgName" value="" validate="{required:true,maxlength:50}"></td>    			</tr>    		</table>    		<input type="button" onclick="f_save()" class="l-button" value="保存">        	    	</form>		<input type="hidden" id="ownOrgId" value="${ownOrgId }">		<input type="hidden" id="orgLev" value="${orgLev }">	</body></html>
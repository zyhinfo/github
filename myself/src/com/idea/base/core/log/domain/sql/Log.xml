<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMap          PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"          "http://ibatis.apache.org/dtd/sql-map-2.dtd"><sqlMap namespace="Log">	<select id="queryLinkUrlAndDesc" resultClass="java.util.HashMap">		select LINK_URL,LINK_DESC from t01_sys_log_setting	</select>	<insert id="insertSysLog" parameterClass="java.util.HashMap">		INSERT INTO T01_SYS_LOG (LOG_ID,USER_ID,USER_NAME,LOG_DESC,OPERATOR_TIME) 		VALUES ('$logId$','$userId$','$userName$','$logDesc$',sysdate)	</insert></sqlMap>
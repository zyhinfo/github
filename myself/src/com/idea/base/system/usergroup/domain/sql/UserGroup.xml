<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMap          PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"          "http://ibatis.apache.org/dtd/sql-map-2.dtd"><sqlMap namespace="UserGroup">	<typeAlias alias="UserGroupBean" type="com.idea.base.system.usergroup.domain.UserGroupBean"/>	<select id="getUserGroupJSON" resultClass="java.util.HashMap" parameterClass="UserGroupBean">		select USERGROUP_ID,USERGROUP_NAME,USERGROUP_DESC  		  from T01_SYS_USERGROUP where 1=1		  <dynamic>		  	<isNotEmpty property="userGroupName">		  		and USERGROUP_NAME like '%'||#userGroupName#||'%'		  	</isNotEmpty>		  </dynamic>	</select>		<insert id="saveUserGroupAdd" parameterClass="UserGroupBean">		insert into T01_SYS_USERGROUP (USERGROUP_ID,USERGROUP_NAME,USERGROUP_DESC)		VALUES ('$userGroupId$', '$userGroupName$', '$userGroupDesc$')			</insert>	<update id="saveUserGroupEdit" parameterClass="UserGroupBean">		update T01_SYS_USERGROUP set 			USERGROUP_NAME='$userGroupName$', 			USERGROUP_DESC='$userGroupDesc$'		where USERGROUP_ID='$userGroupId$'	</update></sqlMap>
<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMap          PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"          "http://ibatis.apache.org/dtd/sql-map-2.dtd"><sqlMap namespace="LogSetting">	<typeAlias alias="LogSettingBean" type="com.idea.base.system.log.logSetting.domain.LogSettingBean"/>	<select id="getLogSettingJSON" resultClass="java.util.HashMap" parameterClass="LogSettingBean">		select LINK_ID,LINK_DESC,LINK_URL from T01_SYS_LOG_SETTING where 1=1		  <dynamic>		  	<isNotEmpty property="linkDesc">		  		and LINK_DESC like '%'||#linkDesc#||'%'		  	</isNotEmpty>		  </dynamic>		  order by link_url,LINK_ID desc	</select>	<select id="getLogSettingJSONCount" resultClass="java.lang.Integer" parameterClass="LogSettingBean">		select count(1) from T01_SYS_LOG_SETTING where 1=1		  <dynamic>		  	<isNotEmpty property="linkDesc">		  		and LINK_DESC like '%'||#linkDesc#||'%'		  	</isNotEmpty>		  </dynamic>	</select>	<insert id="saveLogSettingAdd" parameterClass="LogSettingBean">		insert into T01_SYS_LOG_SETTING (LINK_ID,LINK_DESC,LINK_URL)		VALUES ('$linkId$', '$linkDesc$', '$linkUrl$')			</insert>	<update id="saveLogSettingEdit" parameterClass="LogSettingBean">		update T01_SYS_LOG_SETTING set 			LINK_DESC='$linkDesc$', 			LINK_URL='$linkUrl$'		where LINK_ID='$linkId$'	</update></sqlMap>